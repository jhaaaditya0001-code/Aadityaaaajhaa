package week7.week7workshop;


public class Laptop
{
  String brand;
  int RAM;
  double price;

  public Laptop(String br , int r , double pr){
      this.brand = br;
      this.RAM = r;
      this.price = pr;
  }

  void Display(){
        System.out.println("Brand: " + brand);
        System.out.println("RAM: " + RAM + "GB");
        System.out.println("Price: " + price);
        System.out.println("---------------------");
  }


}
